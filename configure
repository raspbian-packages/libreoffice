        fi
    else
        { { $as_echo "$as_me:$LINENO: error: Java not found. You need at least jdk-1.5, or gcj-4" >&5
$as_echo "$as_me: error: Java not found. You need at least jdk-1.5, or gcj-4" >&2;}
   { (exit 1); exit 1; }; }
    fi
